{% extends "layout.njk" %}
{% import "macros.njk" as macros %}

{% block main %}
{% if not typeDefinition.imports.isEmpty() %}
{{ typeDefinition.imports.toString({ types: '.' }) }}
{% endif %}

{{ macros.docblock(docs) }}
{% if typeDefinition.isEnum %}
export enum {{ name }} {{ typeDefinition.type }};
{% else %}
export type {{ name }} = {{ typeDefinition.type }};
{% endif %}
{% endblock %}

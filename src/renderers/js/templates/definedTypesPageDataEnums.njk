{% if definedType.type.nodeClass === 'TypeEnumNode' and definedType.type.isDataEnum() %}
// Data Enum Helpers.
{% for variant in definedType.type.variants %}
{% if variant.nodeClass === 'TypeEnumStructVariantNode' %}
export function {{ definedType.name | camelCase }}(kind: '{{ variant.name }}', data: GetDataEnumKindContent<{{ definedType.name | pascalCase }}, '{{ variant.name }}'>): GetDataEnumKind<{{ definedType.name | pascalCase }}, '{{ variant.name }}'>;
{% elif variant.nodeClass === 'TypeEnumTupleVariantNode' %}
export function {{ definedType.name | camelCase }}(kind: '{{ variant.name }}', data: GetDataEnumKindContent<{{ definedType.name | pascalCase }}, '{{ variant.name }}'>['fields']): GetDataEnumKind<{{ definedType.name | pascalCase }}, '{{ variant.name }}'>;
{% else %}
export function {{ definedType.name | camelCase }}(kind: '{{ variant.name }}'): GetDataEnumKind<{{ definedType.name | pascalCase }}, '{{ variant.name }}'>;
{% endif %}
{% endfor %}
export function {{ definedType.name | camelCase }}<K extends {{ definedType.name | pascalCase }}['__kind']>(
  kind: K,
  data?: any,
): {{ definedType.name | pascalCase }} & { __kind: K } {
  return Array.isArray(data) ? { __kind: kind, fields: data } : { __kind: kind, ...(data ?? {}) };
}
export function is{{ definedType.name | pascalCase }}<K extends {{ definedType.name | pascalCase }}['__kind']>(
  kind: K,
  value: {{ definedType.name | pascalCase }},
): value is {{ definedType.name | pascalCase }} & { __kind: K } {
  return value.__kind === kind;
};
{% endif %}
